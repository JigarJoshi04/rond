{
    "paths": {
        "/users-from-static-file/": {
            "get": {
                "x-permission": {
                    "allow": "foobar",
                    "resourceFilter": {
                        "resourceType": "custom",
                        "rowFilter": {
                            "headerKey": "customHeaderKey"
                        }
                    }
                }
            },
            "post": {
                "x-permission": {
                    "allow": "notexistingpermission"
                }
            }
        },
        "/no-permission-from-static-file": {
            "post": {}
        }
    }
}